#!/bin/sh

picom --experimental-backends &
xrdb merge ~/.Xresources &
feh --bg-fill ~/pictures/wallpaper/marine\ drunk.jpg &
dunst &
xautolock -time 30 -locker 'systemctl hybrid-sleep' -notify 60 -notifier "notify-send -u critical 'SYSTEM GOING TO SLEEP' 'in 60 seconds'" &
fcitx5 &

# run bar script and dwm ( do not remove this )
~/.config/iamchad/scripts/./bar.sh &
dwm
